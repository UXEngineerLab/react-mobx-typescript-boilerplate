{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "target": "esnext",
      "module": "es6",
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "moduleResolution": "node",
      "noImplicitThis": true,
      "strict": true
    }
  }
